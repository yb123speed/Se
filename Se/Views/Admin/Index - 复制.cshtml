    <style type="text/css">

        .shareLi li {
            float: left;
            width: 135px;
            border-radius: 5px;
            margin: 25px 10px;
            display: inline-block;
        }

            .shareLi li.zudou {
                background: #22DCC4;
            }
    </style>
    <script type="text/javascript">
        $(function () {


//            changeMenu(4);

            //如果图片不存在，设置默认图片
            $("img").each(function (i, e) {
                var imgsrc = $(e).attr("src");
                $(e).load(function () {
                    $("<p> ok " + $(e).width() + ":" + $(e).height() + "</p>").appendTo("#msgTool");
                }).error(function () {
                    $("<p> error " + imgsrc + "</p>").appendTo("#msgTool");
                    $(e).attr("src", "/images/img400.jpg");
                });

            });
        });


    </script>
</head>
